{
  "exclude": [
    "node_modules",
    "./dist",
    "src/test"
  ],
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": false,
    "allowSyntheticDefaultImports": false,
    "allowUnusedLabels": true
  }
}